{
	"compilerOptions": {
		"jsx": "react-jsx",
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"checkJs": false,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"strict": false,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"types": ["vite/client"]
	},
	"include": ["src"]
}
